        user_id = await get_identifier(str(telegram_id))
        
        # APIë¡œ í˜„ì¬ ì„¤ì • ê°€ì ¸ì˜¤ê¸°
        settings = await get_dual_side_settings_api(user_id)
        
        # ì„¤ì • ì—…ë°ì´íŠ¸
